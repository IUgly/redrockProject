server:
  port: 8020
mybatis:
  config-location: classpath:mybatis/mybatis.cfg.xml    # mybatis配置文件所在路径
  type-aliases-package: team.redrock.template.vo            # 定义所有操作类的别名所在包
  mapper-locations:                                     # 所有的mapper映射文件
  - classpath:mybatis/mapper/**/*.xml
spring:
  datasource:
    url: jdbc:mysql://?useSSL=false&useUnicode=true&characterEncoding=UTF-8&useJDBCCompliantTimezoneShift=true&useLegacyDatetimeCode=false&serverTimezone=PRC
    username:
    password:
    driver-class-name: com.mysql.jdbc.Driver
    max-idle: max-idle
    max-wait: 1000
    min-idle: 5
    initial-size: 5
  redis:
    host: localhost
    port: 6379
    password:
    timeout: 1000
    database: 0
    pool:
      max-active: 10
      max-idle: 8
      min-idle: 2
      max-wait: 100
  http:
    multipart:
      enabled: true

